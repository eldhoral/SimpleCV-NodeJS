<!-- CONTACT -->
<body>
    <div id="contact">
        <div class="contact">
            <div class="getintouch">
                <h1>Get in touch</h1>
                <br>
                <p>Hey what's up?. Go ahead and drop me a line. You can also choose to find me as
                    <strong>@eldhorizcky</strong> elsewhere!</p>
                <ul class="socialmedia">
                    <li class="socmed">
                        <a href="https://www.linkedin.com/in/eldhorizcky/" target="_blank">
                            <img src="assets/link.png" alt="">
                        </a>
                    </li>
                    <li class="socmed">
                        <a href="https://www.instagram.com/eldhorizcky" target="_blank">
                            <img src="assets/ig.png" alt="">
                        </a>
                    </li>
                    <li class="socmed">
                        <a href="https://www.github.com/eldhoral" target="_blank">
                            <img src="assets/git.png" alt="">
                        </a>
                    </li>
                </ul>
            </div>
            <div class="contact-form">
                <form action="#" method="PUSH">
                    <label style="letter-spacing: 5px;" for="">WRITE TO ME!</label>
                    <input type="text" id="name" required placeholder="Y O U R  N A M E">
                    <input type="email" id="email" required placeholder="Y O U R  E M A I L">
                    <label style="letter-spacing: 5px;" for="">MESSAGE</label>
                    <textarea name="message" id="message" rows="6" required></textarea>
                    <input type="submit" name="submit" id="submit" value="SEND" >
                </form>
            </div>
        </div>
    </div>
</body>



